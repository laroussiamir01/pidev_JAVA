<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="300.0" style="-fx-background-color: #e9dee6;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.User.ChatBotController">
    <children>
        <Label fx:id="lblTitle" layoutX="431.0" layoutY="14.0" text="Chat Bot">
            <font>
                <Font name="System Bold Italic" size="18.0" />
            </font>
        </Label>
        <TextField fx:id="txtInput" layoutX="196.0" layoutY="72.0" prefHeight="25.0" prefWidth="272.0" />
        <Button fx:id="btnSend" layoutX="483.0" layoutY="72.0" mnemonicParsing="false" style="-fx-background-color: black;" text="Send" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
        <Label fx:id="lblOutput" layoutX="196.0" layoutY="107.0" prefHeight="800.0" prefWidth="1200.0" text="Output" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
      <AnchorPane layoutY="-5.0" prefHeight="918.0" prefWidth="190.0" style="-fx-background-color: #5CE1E6;">
         <children>
            <Button layoutX="49.0" layoutY="33.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="83.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Back" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
