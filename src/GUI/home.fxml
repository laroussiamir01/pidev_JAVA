<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.HomeController">
    <stylesheets>
        <URL value="@home.css" />
    </stylesheets>
   <children>
      <VBox layoutY="3.0" prefHeight="600.0" prefWidth="130.0" styleClass="mainFxmlClass">
         <children>
            <Label prefHeight="66.0" prefWidth="136.0" text="home :" textAlignment="CENTER">
               <font>
                  <Font size="29.0" />
               </font></Label>
            <Button fx:id="analyse" mnemonicParsing="false" onMouseClicked="#analyseButton" prefHeight="50.0" prefWidth="150.0" style="-fx-text-fill: black;" styleClass="menuItem" text="Analyse" />
            <Button fx:id="laboratoire" mnemonicParsing="false" onMouseClicked="#laboratoireButton" prefHeight="50.0" prefWidth="150.0" style="-fx-text-fill: black;" styleClass="menuItem" text="Laboratoire" />
            <Button fx:id="rating" mnemonicParsing="false" onMouseClicked="#ratingButton" prefHeight="50.0" prefWidth="150.0" style="-fx-text-fill: black;" styleClass="menuItem" text="Rating" />
         </children>
      </VBox>
      <TabPane fx:id="anchorPane" layoutX="130.0" layoutY="4.0" prefHeight="600.0" prefWidth="850.0" styleClass="mainFxmlClass" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="ajouter" text="Ajouter" />
          <Tab fx:id="consulter" text="Consulter" />
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
